	and DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(actual.sales_month,'01'), '%Y%m%d'), INTERVAL 1 year), '%Y%m') = DATE_FORMAT(STR_TO_DATE(CONCAT(prior.sales_month,'01'), '%Y%m%d'), '%Y%m') 
