    <sql> -- TRUNCATE TABLE  fine_dm.dm_model_mm_vpm

 delete from  fine_dm.dm_model_mm_vpm
 -- where  report_year ='2022'
 --      and ( sales_month in ('202201' ,'202202','202203','202204','202205','202206','202207' ,'202208','202209','202210','202211','202212')
 --            or sales_month in ('202101' ,'202102','202103','202104','202105','202106','202107' ,'202108','202109','202110','202111','202112')
 --            )
where  report_year = SUBSTRING(${mysql_yesterday_l_month},1,4)
  and order_no >= 191</sql>
