			,sum(ly_d_qty) as ly_d_qty
			,sum(ly_d_pc) as ly_d_pc
			,sum(ly_d_sales) as ly_d_sales

FROM(
     SELECT     
     -- 维度字段
			 order_type
			,channel
			,customer_code
			,customer_name
			,proj_name
			,proj_name_en
			,item_code 
