    <sql>-- 20240902修改ytd增加sals%
-- 注意同期月份数
/*
-- 368340
-- SELECT * FROM  fine_dm.dm_model_distributor_vpm
TRUNCATE TABLE fine_dm.dm_model_distributor_vpm;
INSERT INTO fine_dm.dm_model_distributor_vpm
(
	order_type,
	channel,
	customer_code,
	customer_name,
	proj_name,
	proj_name_en,
	item_code,
	item_code_ppg,
	category,
	category_brand,
	category_product_type,
	sales_value,
	sales_month,
	report_year,
	order_no,
	sec_1,
	sec_2,
	sec_3,
	report_date,
	etl_time
)
*/

-- TRUNCATE TABLE fine_dm.dm_model_distributor_vpm;
-- INSERT INTO fine_dm.dm_model_distributor_vpm
-- (
-- 	order_type,
-- 	channel,
-- 	customer_code,
-- 	customer_name,
-- 	proj_name,
-- 	proj_name_en,
-- 	item_code,
-- 	item_code_ppg,
-- 	category,
-- 	category_brand,
-- 	category_product_type,
-- 	sales_value,
-- 	sales_month,
-- 	report_year,
-- 	order_no,
-- 	sec_1,
-- 	sec_2,
-- 	sec_3,
-- 	report_date,
-- 	etl_time
-- )

with temp_dw_order_report as (
select * 
	 from fine_dw.dw_order_report orderno
	 where orderno.order_report = 'model_vpm'
				 and  orderno.report_year = SUBSTRING(${mysql_yesterday_l_month},1,4)
 ),temp_d_dw_price_increase as(
		SELECT -- count(1) -- 334050
			'进货' order_type,
			t.customer_code,
			t.customer_name,
			case when proj_name = '比亚迪' then item_code_ppg else item_code end as item_code,
			t.item_code_ppg,
			t.category,
			t.category_brand,
			t.category_product_type,
			t.channel,
			t.proj_name,
			t.proj_name_en,
			-- t.sales_month,
			t.sales_year,
			sum(t.d_volume) as d_volume,
			sum(t.d_sales) as d_sales,
			sum(t.d_qty) as d_qty,
			sum(t.d_pc) as d_pc
			-- DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(sales_month,'01'), '%Y%m%d'), INTERVAL 1 year), '%Y%m') as sales_month_2
		FROM 
			fine_dw.dw_price_increase t
		where 1=1 and
		(
	     	SUBSTRING(sales_month,1,4) = SUBSTRING( ${mysql_yesterday_l_month},1,4)
		  and sales_month &lt;= ${mysql_yesterday_l_month}  )
		and t.channel = 'DISTRIBUTOR'
-- 		and t.channel &lt;&gt; 'SCHOOL'
		-- and proj_name = '一汽丰田'
		-- and item_code = 'P991-8952.TOY/1L-C3'
        Group BY  
			t.customer_code,
			t.customer_name,
			case when proj_name = '比亚迪' then item_code_ppg else item_code end ,
			t.item_code_ppg,
			t.category,
			t.category_brand,
			t.category_product_type,
			t.channel,
			t.proj_name,
			t.proj_name_en,
			-- t.sales_month,
			t.sales_year
			-- DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(sales_month,'01'), '%Y%m%d'), INTERVAL 1 year), '%Y%m')
UNION ALL
		SELECT -- count(1) -- 334050
			'备货' order_type,
			t.customer_code,
			t.customer_name,
			case when proj_name = '比亚迪' then item_code_ppg else item_code end as item_code,
			t.item_code_ppg,
			t.category,
			t.category_brand,
			t.category_product_type,
			t.channel,
			t.proj_name,
			t.proj_name_en,
			-- t.sales_month,
			t.sales_year,
			sum(-1*t.s_volume) as s_volume,
			sum(-1*t.s_sales) as s_sales,
			sum(-1*t.s_qty) as s_qty,
			sum(-1*t.s_pc) as s_pc
			-- DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(sales_month,'01'), '%Y%m%d'), INTERVAL 1 year), '%Y%m') as sales_month_2
		FROM 
			fine_dw.dw_price_increase t
		where 1=1
	  and 		   
	  ( SUBSTRING(sales_month,1,4) = SUBSTRING( ${mysql_yesterday_l_month},1,4)
