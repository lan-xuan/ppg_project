		  	, null as ly_d_qty
			, null as ly_d_pc
			, null as ly_d_sales
	
			FROM temp_d_dw_price_increase  -- 当年数据
union all 
   SELECT     
     -- 维度字段
			 order_type
			,channel
			,customer_code
			,customer_name
			,proj_name
			,proj_name_en
			,item_code 
