			,proj_name
			,proj_name_en
			,item_code 
			,item_code_ppg 
			,category
			,category_brand
			,category_product_type

    -- 日期字段
      		-- ,sales_month
			,sales_year
			-- ,sales_month_2
	-- 销售数据
			,sum(d_volume) as d_volume
			,sum(d_qty) as d_qty
			,sum(d_pc) as d_pc
			,sum(d_sales) as d_sales
