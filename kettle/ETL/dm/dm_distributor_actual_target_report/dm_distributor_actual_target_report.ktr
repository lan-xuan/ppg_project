            'dm_distributor_target_report/dm_distributor_report' as data_resource,
            now() as etl_time,
            report_date

                        
from t
            LEFT JOIN fine_dw.dw_order_report orderno
            on upper(orderno.name_1) = upper(t.sales_target_name)
            and orderno.order_month = t.sales_month
