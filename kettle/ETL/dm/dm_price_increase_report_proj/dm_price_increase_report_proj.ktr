,bb as(
SELECT sales_year,sales_month,customer_code,customer_name,proj_name,proj_name_en,channel,d_qty as sales_value,'Qty' as sec_1,'Distributor Sales' as sec_2,CONCAT('FY',substr(sales_year,3,2),' ',sales_month_en) as sec_3,'是' as is_flag FROM aa WHERE 1=2


