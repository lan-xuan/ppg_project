sum((net_sales - ly_net_sales) 
- if( COALESCE(d_volume,0)*COALESCE(ly_d_volume,0)=0 ,0,COALESCE(((d_sales/ NULLIF(d_volume, 0)) - (ly_d_sales/NULLIF(ly_d_volume, 0)))*d_volume,0)) 
