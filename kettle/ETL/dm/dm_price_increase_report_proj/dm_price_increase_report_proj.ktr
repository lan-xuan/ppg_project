GROUP BY sales_year,sales_month,customer_code,customer_name,proj_name,proj_name_en,channel,sales_month_en

UNION ALL
-- Price Increase (Distributor) &amp; Impact -- 1-2 明细表的(3-1) + (4-1)  -- 明细表7.1dm_price_increase_report
-- 3-1
-- if(net_sales/NULLIF(net_volume, 0)*ly_net_sales/NULLIF(ly_net_volume, 0)=0 ,0,COALESCE(((d_sales/ NULLIF(d_volume, 0)) - (ly_d_sales/NULLIF(ly_d_volume, 0)))*d_volume,0))
-- 4-1
-- if(s_sales*s_volume*ly_s_sales*ly_s_volume=0,0, (-1 * ((s_sales/NULLIF(s_volume, 0) - ly_s_sales/NULLIF(ly_s_volume, 0))) * s_volume))
SELECT sales_year,sales_month,customer_code,customer_name,proj_name,proj_name_en,channel,
sum(
if( COALESCE(d_volume,0)*COALESCE(ly_d_volume,0)=0 ,0,COALESCE(((d_sales/ NULLIF(d_volume, 0)) - (ly_d_sales/NULLIF(ly_d_volume, 0)))*d_volume,0))
+
if(s_sales*s_volume*ly_s_sales*ly_s_volume=0,0, (-1 * ((s_sales/NULLIF(s_volume, 0) - ly_s_sales/NULLIF(ly_s_volume, 0))) * s_volume))
)
,'PRICE' as sec_1,'IMPACT' as sec_2,sales_month_en as sec_3,'是' as is_flag
