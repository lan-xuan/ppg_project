SELECT sales_year,sales_month,customer_code,customer_name,proj_name,proj_name_en,channel,sum(net_sales),'GROSS SALES' as sec_1,'ACTUAL' as sec_2,upper(sales_month_en) as sec_3,'GROSSSALES' as is_flag FROM aa
