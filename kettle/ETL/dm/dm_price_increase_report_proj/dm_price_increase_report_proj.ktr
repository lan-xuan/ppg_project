    <sql>-- TRUNCATE TABLE  fine_dm.dm_price_increase_report_proj

 delete from  fine_dm.dm_price_increase_report_proj
 -- where  report_year ='2023'
 --     and ( sales_month in ('202301' ,'202302','202303','202304','202305','202306','202307' ,'202308','202309','202310','202311','202312')
 --          or sales_month in ('202201' ,'202202','202203','202204','202205','202206','202207' ,'202208','202209','202210','202211','202212')
 --         )
 where  report_year = SUBSTRING(${mysql_yesterday_l_month},1,4)
 and ( sales_month = ${mysql_yesterday_l_month}  or sales_month = ${mysql_yesterday_l_month_l_year} )</sql>
