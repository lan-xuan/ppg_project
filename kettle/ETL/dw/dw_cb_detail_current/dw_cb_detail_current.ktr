-- 										and item_code = 'P426-PP05.HX/1L-C3'
										and DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(substr(service_rate_starting_date,1,6),'01'), '%Y%m%D'), INTERVAL 0 MONTH), '%Y%m') 
