-- 										and item_code in ('P190-588/5L-C3','P190-588.HX/5L-C3')
										and DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(substr(service_fee_starting_date,1,6),'01'), '%Y%m%D'), INTERVAL 0 MONTH), '%Y%m') 
