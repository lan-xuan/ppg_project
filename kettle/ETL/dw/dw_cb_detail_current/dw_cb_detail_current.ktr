				  DATE_FORMAT(DATE_SUB(STR_TO_DATE(concat(substr(commision_fee_ending_date,1,6),'01'), '%Y%m%D'), INTERVAL 0 MONTH), '%Y%m')   as bs_ending_date_2
				from 
				
